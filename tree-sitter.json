{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "haml",
      "camelcase": "HAML",
      "title": "HAML Parser",
      "scope": "source.haml",
      "file-types": [
        "haml"
      ],
      "injection-regex": "^haml$",
      "class-name": "TreeSitterHaml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "HAML Parser",
    "authors": [
      {
        "name": "Bohdan Shushval",
        "email": "bohdanshushval@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/BohdaR/tree-sitter-haml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
